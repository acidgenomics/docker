#!/usr/bin/env bash
set -Eeu -o pipefail

# Build all Acid Genomics images.
# Updated 2019-10-30.

script_dir="$(cd "$(dirname "${BASH_SOURCE[0]}")" >/dev/null 2>&1 && pwd)"
build="${script_dir}/docker-build-all-tags"

# Ensure we're working from a clean Docker installation.
docker system prune --all --force

"$build" bioconductor
"$build" basejump
"$build" bcbiornaseq
"$build" bcbiosinglecell
"$build" rnaseq
"$build" singlecell

# > "$build" rnaeditingindexer
