#!/usr/bin/env bash
set -Eeu -o pipefail

# """
# Push a local build.
# Updated 2020-02-16.
#
# Useful if GPG agent causes push failure.
#
# @examples
# docker-push acidgenomics/debian:latest
# """

image="${1:?}"
server="docker.io"
today=$(date "+%Y%m%d")

docker push "${server}/${image}-${today}"
docker push "${server}/${image}"
